{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "insertPragma": false,
  "jsxSingleQuote": false,
  "singleAttributePerLine": true,
  "proseWrap": "preserve",
  "requirePragma": false,
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "css",
  "overrides": [
    {
      "files": [
        "*.mts",
        "*.cts",
        "*.ts"
      ],
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": [
        "*.mjs",
        "*.cjs",
        "*.js"
      ],
      "options": {
        "parser": "babel"
      }
    },
    {
      "files": [
        "*.json",
        "*.jsonc",
        "*.json5"
      ],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.md",
      "options": {
        "parser": "markdown",
        "proseWrap": "always"
      }
    }
  ]
}
